Cell[TextData[{
 StyleBox["d.",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[" An unskilled worker earns the marginal product of labor, whereas \
a skilled worker earns the marginal product of labor plus the marginal \
product of human capital. Using your answers to parts (a) and (b), find the \
ratio of the skilled wage to the unskilled wage. How does an increase in the \
amount of human capital affect this ratio? Explain.",
  FontSlant->"Italic"]
}], "Text",
 CellChangeTimes->{{3.7611445813085523`*^9, 3.761144712427426*^9}, {
  3.761144829421776*^9, 
  3.761144829421776*^9}},ExpressionUUID->"ad5b6558-7574-4a11-a5ca-\
cfd8ffd07c8f"],

Cell[TextData[{
 "\tFor a skilled worker, his unit wage is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"MPL", "+", "MPH"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b8689a19-20d7-42f9-92a0-b027182eb889"],
 ", while unskilled one is ",
 Cell[BoxData[
  FormBox["MPL", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2d2c4d3a-6e2a-4a4f-8f53-688cd545e19f"],
 ", the ratio between them is:"
}], "Text",
 CellChangeTimes->{{3.7611445813085523`*^9, 3.761144712427426*^9}, {
  3.7611448309402714`*^9, 3.761144927410471*^9}, {3.7611451380718784`*^9, 
  3.76114513830229*^9}},ExpressionUUID->"01225403-586c-4333-9f21-\
ffc19373a3cb"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"MPL", "+", "MPH"}], "MPL"], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["H", 
         RowBox[{"1", "/", "3"}]], " ", 
        SuperscriptBox["K", 
         RowBox[{"1", "/", "3"}]]}], 
       RowBox[{"3", " ", 
        SuperscriptBox["L", 
         RowBox[{"2", "/", "3"}]]}]], "+", 
      FractionBox[
       RowBox[{
        SuperscriptBox["K", 
         RowBox[{"1", "/", "3"}]], " ", 
        SuperscriptBox["L", 
         RowBox[{"1", "/", "3"}]]}], 
       RowBox[{"3", " ", 
        SuperscriptBox["H", 
         RowBox[{"2", "/", "3"}]]}]]}], 
     FractionBox[
      RowBox[{
       SuperscriptBox["H", 
        RowBox[{"1", "/", "3"}]], " ", 
       SuperscriptBox["K", 
        RowBox[{"1", "/", "3"}]]}], 
      RowBox[{"3", " ", 
       SuperscriptBox["L", 
        RowBox[{"2", "/", "3"}]]}]]], "=", 
    FractionBox[
     RowBox[{"H", "+", "L"}], "H"]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7611445813085523`*^9, 3.761144712427426*^9}, {
  3.7611448309402714`*^9, 
  3.761144975612218*^9}},ExpressionUUID->"ebcc12d1-4b83-47a8-b374-\
74ec0fb677c7"],

